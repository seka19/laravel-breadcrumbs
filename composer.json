{
  "name": "seka19/laravel-breadcrumbs",
  "description": "Laravel package for displaying DB-based breadcrumbs in views",
  "type": "package",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Alexey Sinkevich",
      "email": "mail@neoways.ru"
    }
  ],
  "require": {
    "php": ">=7.4",
    "illuminate/console": "^8",
    "illuminate/container": "^8",
    "illuminate/contracts": "^8",
    "illuminate/database": "^8",
    "illuminate/http": "^8",
    "illuminate/support": "^8"
  },
  "require-dev": {
    "laravel/framework": "^8"
  },
  "autoload": {
    "psr-4": {
      "Seka19\\LaravelBreadcrumbs\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Seka19\\LaravelBreadcrumbs\\LaravelBreadcrumbsServiceProvider"
      ]
    }
  }
}
